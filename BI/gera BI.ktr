    <sql>use carrefour;
SELECT 
	sd.JTS_OID, 
	   SD.C1632,
	   SD.C1633,
--	   sc.CNPJ               				AS CNPJ_Loja,
99999999999999 AS CNPJ_Loja,
--       sc.NOMBRECIUDAD       				AS Municipio_Loja,
'MUNICIPIO_LOJA_?' AS Municipio_Loja,
 --      sc.UF                 				AS UF_Loja,
'UF_LOJA_?' AS UF_Loja,
 --      sc.CODIGOPOSTAL       				AS CEP_Loja,
99999999 AS CEP_Loja,
       cl.NOMBRECLIENTE      				AS Nome,
       fj.NUMERODOCUMENTO    				AS CPF,
       en.TIPO_LOG + ' ' + en.LOGRADOURO  	AS Logradouro,
       en.NUMERO							AS Numero_Residencia,
       en.COMPLEMENTO						AS Complemento_Residencia,
       en.CODIGOPOSTAL						AS CEP_Residencia,
       en.BAIRRO							AS Bairro_Residencia,
       en.CIDADE							AS Cidade_Residencia,
       en.UF 								AS UF_Residencia,	
       fr.RENDA_BRUTA * 100					AS Renda,
       ((sd.C1604 * (-1)) + sd.C1609) * 100 AS Sld_Dev_Ctb,
/* ------
	Selects com SUM
   ------ */
       sd.OPERACION							AS Nro_Contrato,
       'ATIVO'								AS Status_Contrato,
       0									AS Vlr_Refinanc,
       'DESCONTO EM FOLHA'					AS Tipo_Pagto,
       sd.C1730								AS Conta_Ctb, 
       'D' 									AS Deb_Cred,
       (SELECT sum(pp.C2309 + pp.C2310)
          FROM PLANPAGOS pp
         WHERE sd.JTS_OID = pp.SALDO_JTS_OID
           AND pp.C2309 + pp.C2310 != 0) * 100
                             				AS Vlr_Contr_Futuro,
       (SELECT sum(isnull(de.CAPITALADELANTADO, 0) + isnull(de.CAPITALPAGADO, 0))
          FROM PLANPAGOS pp
               LEFT OUTER JOIN BS_PAYS_DETAIL de    ON sd.JTS_OID = de.SALDOS_JTS_OID AND
                                                       pp.C2300   = de.CUOTA          AND
                                                       de.CAMBIA_IMPORTE_SALDO_CUOTA = 'S' AND
                                                       de.TZ_LOCK = 0
         WHERE sd.JTS_OID = pp.SALDO_JTS_OID
           AND pp.C2309 + pp.C2310 = 0) * 100
                             				AS Vlr_Pago_Princ,
       (SELECT sum(de.CAPITALADELANTADO + de.CAPITALPAGADO + de.INTERESPAGADO)
          FROM PLANPAGOS pp
               LEFT OUTER JOIN BS_PAYS_DETAIL de    ON sd.JTS_OID = de.SALDOS_JTS_OID AND
                                                       pp.C2300   = de.CUOTA          AND
                                                       de.CAMBIA_IMPORTE_SALDO_CUOTA = 'S' AND
                                                       de.TZ_LOCK = 0
         WHERE sd.JTS_OID = pp.SALDO_JTS_OID
           AND pp.C2309 + pp.C2310 = 0
           AND de.FECHAVALOR &lt; pp.C2302_ORIGINAL) * 100
                             				AS Vlr_Parc_Antec,
       (SELECT count(*)
          FROM PLANPAGOS pp
               LEFT OUTER JOIN BS_PAYS_DETAIL de    ON sd.JTS_OID = de.SALDOS_JTS_OID AND
                                                       pp.C2300   = de.CUOTA          AND
                                                       de.CAMBIA_IMPORTE_SALDO_CUOTA = 'S' AND
                                                       de.TZ_LOCK = 0
         WHERE sd.JTS_OID = pp.SALDO_JTS_OID
           AND pp.C2309 + pp.C2310 = 0
           AND de.FECHAVALOR &lt; pp.C2302_ORIGINAL)
                             				AS Qtd_Parc_Antec,
       CONVERT(VARCHAR, sd.C1620 , 103)		AS Dt_Contrato,
       CASE
         WHEN (SD.C1604 = 0 AND PZ.MOTIVO_BAJA = 1) THEN 'DESACORDO COMERCIAL'
         WHEN (SD.C1604 = 0 AND PZ.MOTIVO_BAJA = 2) THEN 'CONDICOES CONTRATACAO'
         WHEN (SD.C1604 = 0 AND PZ.MOTIVO_BAJA = 3) THEN 'CONTRATACAO INDEVIDA'
		 ELSE '' 
       END 									AS Motivo_Cancelamento,
       CASE
         WHEN (CE.TIPOESPECIE = 3) THEN 'APOSENTADO'
         WHEN (CE.TIPOESPECIE = 8) THEN 'PENSIONISTA'
         ELSE 'OUTROS'
       END 									AS Ocupacao,  
       sd.C1612 * 100            			AS Vlr_Mensal_Parcela,
       convert(varchar, (SELECT fechaprocesoanterior FROM PARAMETROS), 103) 
                             				AS Dt_Referencia,
       '${TOPAZ_FILE_OUTPUT}' + '/INTEGRACAO_BI_' + convert(varchar, convert(date, (SELECT fechaprocesoanterior FROM PARAMETROS)) )
                             				AS Nome_arquivo,
       convert(varchar, getdate(), 103) 
                             				AS Dt_Sistema
FROM SALDOS sd
       INNER JOIN SUCURSALES sc ON sd.SUCURSAL = sc.SUCURSAL AND sc.TZ_LOCK = 0
-- Viviane
        LEFT JOIN PZO_SALDOS PZ ON SD.JTS_OID = PZ.SALDO_JTS_OID
         AND PZ.TZ_LOCK = 0
       INNER JOIN CLI_CLIENTES cl ON sd.C1803 = cl.CODIGOCLIENTE
       INNER JOIN CLI_CLIENTEPERSONA cp ON cl.CODIGOCLIENTE = cp.CODIGOCLIENTE
       INNER JOIN CLI_DOCUMENTOSPFPJ fj ON cp.NUMEROPERSONA = fj.NUMEROPERSONAFJ AND
                                           fj.TIPODOCUMENTO = 'CPF' AND
                                           fj.TZ_LOCK       = 0
-- Viviane
   	   LEFT JOIN PPN_MATRICULA MA ON MA.MATRICULABENEFICIARIO = SD.MATRICULA
         AND MA.TZ_LOCK = 0
         AND MA.CODIGOCONVENIO = SD.C1772
       LEFT JOIN PPN_CODIGOS_ESPECIES CE ON CE.CODIGOESPECIE = MA.CODIGOESPECIE
        AND CE.TZ_LOCK = 0 

--       LEFT OUTER JOIN CLI_ENDERECO_PESSOA en ON en.NUMEROPESSOA = cp.NUMEROPERSONA AND en.TZ_LOCK = 0
		LEFT JOIN CLI_ENDERECO_PESSOA EN ON EN.NUMEROPESSOA = fj.NUMEROPERSONAFJ  -- Tabela CLI_DOCUMENTOSPFPJ
         AND EN.TIPOENDERECO = 1
         AND EN.TZ_LOCK = 0      
         AND EN.ORDINAL = (SELECT MAX(ENDE.ORDINAL)
                            FROM dbo.CLI_ENDERECO_PESSOA ENDE
                           WHERE ENDE.NUMEROPESSOA = EN.NUMEROPESSOA
                             AND ENDE.TIPOENDERECO = 1
                           GROUP BY ENDE.NUMEROPESSOA) 

       LEFT OUTER JOIN CLI_FONTE_RENDA fr ON cp.NUMEROPERSONA = fr.NRO_PESSOA AND fr.TZ_LOCK = 0
 WHERE sd.C1785 = 5
   AND sd.C1604 != 0
ORDER BY sd.JTS_OID</sql>
