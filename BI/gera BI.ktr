    <connection>TopazDS</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT 
         (CASE
            WHEN PP.C2302_original >= (SELECT fechaprocesoanterior FROM PARAMETROS) -- a vencer
              THEN ROUND((pp.C2309 + pp.C2310) / (POWER((CAST(? AS DECIMAL(11,7) ) + (26 / 100)), (ABS(datediff(day, (SELECT fechaprocesoanterior FROM PARAMETROS),PP.C2302_original)) / 360))),2)
            ELSE ROUND((pp.C2309 + pp.C2310) * (POWER((CAST(? AS DECIMAL(11,7) ) + (26 / 100)), (ABS(datediff(day, (SELECT fechaprocesoanterior FROM PARAMETROS),PP.C2302_original)) / 360)) - 1),2) + (pp.C2309 + pp.C2310)
 
          END) * 100 AS Sld_Dev_Fin_Detalhe,

           (CASE
            WHEN PP.C2302_original >= (SELECT fechaprocesoanterior FROM PARAMETROS) -- a vencer
              THEN ROUND((pp.C2309 + pp.C2310) / (POWER((CAST(? AS DECIMAL(11,7) ) + (1 / 100)), (ABS(  datediff(day, (SELECT fechaprocesoanterior FROM PARAMETROS),PP.C2302_original)) / 360))),2)
            ELSE ROUND((pp.C2309 + pp.C2310) * (POWER((CAST(? AS DECIMAL(11,7) ) + (1 / 100)), (ABS(  datediff(day, (SELECT fechaprocesoanterior FROM PARAMETROS),PP.C2302_original)) / 360)) - 1),2) + (pp.C2309 + pp.C2310)
          END + isnull(gc.IMPORTE_GASTO, 0) + isnull(id.MONTO_IOF, 0)) * 100 AS Sld_Dados_Antec_Detalhe


     FROM PLANPAGOS pp


               LEFT OUTER JOIN GASTOS_POR_CUOTA gc  ON pp.SALDO_JTS_OID = gc.SALDOS_JTS_OID AND
                                                       pp.C2300   = gc.NUMERO_CUOTA
               LEFT OUTER JOIN BS_IOF_DETAIL id     ON pp.SALDO_JTS_OID = id.SALDOS_JTS_OID AND
                                                       pp.C2300   = id.NUMERO_CUOTA   AND
                                                       id.TZ_LOCK = 0

    WHERE pp.SALDO_JTS_OID = ?
      AND 
	  pp.C2309 + pp.C2310 != 0
      AND pp.TZ_LOCK = 0</sql>
