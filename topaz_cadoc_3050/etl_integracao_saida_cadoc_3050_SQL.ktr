    <sql>WITH DIAS AS (
  SELECT 1 AS LEVEL
  UNION ALL
  SELECT LEVEL + 1
  FROM DIAS 
  WHERE LEVEL &lt; 7 
)
SELECT 
     CASE 
        WHEN MAX(DT.DIA) = ? THEN 1	
        ELSE 0
        END AS DiasUteis
FROM 
(
SELECT 
DATEADD(DAY, + 1, DATEADD(DAY, - LEVEL, EOMONTH(CONVERT(date, ?, 20)))) AS DIA
FROM DIAS
) DT
INNER JOIN VW_FERIADOS_BRASIL2 V ON V.DT_COMPLETO &lt;&gt; DT.DIA AND V.MESFERIADO &lt;&gt; 0
