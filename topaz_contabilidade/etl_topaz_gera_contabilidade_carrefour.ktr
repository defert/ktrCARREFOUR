    <connection>TopazDS</connection>
    <sql>SELECT  MC.OPERACION, MC.COD_TRANSACCION,
	'0004667'											AS COD_EMPRESA,
	'O'													AS COD_TIPO_CONTABIL,
	'00000001'											AS NUM_GUIA,
	'TPZ'												AS COD_SIS,
	REPLICATE(' ', 6)            						AS NUM_LANC,
	'0004668'											AS COD_FILIAL, 
	CONVERT(FLOAT, PC.C6326)							AS COD_CONTA_CONTABIL, 
	'020458'											AS ID_CENTRO_CUSTO,
	REPLICATE(' ', 8)									AS COD_PROJETO,
	FORMAT(MC.FECHAPROCESO,'yyyyMMdd')					AS DT_LANC_CONTABIL,
	MC.DEBITOCREDITO 									AS TIPO_LANC,
	REPLICATE(' ', 8)									AS COD_HIST_PADRAO,
	'006'												AS REFERENCIA,
	REPLICATE(' ', 8)									AS DT_CONVERSAO,
	SUM(CONVERT (FLOAT, (MC.CAPITALREALIZADO * 100)))	AS VLR,
	REPLICATE(' ', 20)									AS COMPLEMENTO,
	TT.DESCRIPCION										AS HIST,
	' '                     							AS IND_RATEIO,
	REPLICATE(' ', 15)           						AS COD_CONTA_CONTRA,
	REPLICATE(' ', 5)            						AS ID_CCUSTO_CONTRA,
	'020458'											AS COD_CENTRO_CUSTO,
	REPLICATE(' ', 15)									AS COD_CENTRO_CUSTO_CONTRA,
	REPLACE('CONTABIL' + '_' +  RIGHT(REPLICATE('0', 4) + 
--		CONVERT(VARCHAR, E.EMPRESA), 4) + '_' + 
		CONVERT(VARCHAR, 0004667), 4) + '_' + 
		CONVERT(VARCHAR(10),(CASE WHEN '${pDATABASE}' = '' THEN (SELECT FECHAPROCESOANTERIOR FROM PARAMETROS ) ELSE CONVERT(DATETIME, '${pDATABASE}', 102) END), 103),'/','') 
		AS NOME_ARQUIVO

FROM MOVIMIENTOS_CONTABLES MC
INNER JOIN EMPRESAS E 
	ON MC.EMPRESA = E.EMPRESA  
INNER JOIN  ASIENTOS A 
    ON  MC.FECHAPROCESO = A.FECHAPROCESO
    AND MC.SUCURSAL = A.SUCURSAL
    AND MC.ASIENTO = A.ASIENTO
    AND A.ESTADO = 77
 INNER JOIN PLANCTAS PC
    ON  PC.C6326 = MC.RUBROCONTABLE
    AND PC.TZ_LOCK = 0
LEFT JOIN  TTR_TRANSACCIONES_SIST_EXT TT 
    ON  TT.CODIGO_OPERACION = MC.OPERACION
    AND TT.CODIGO_TTR = MC.COD_TRANSACCION
    AND TT.SISTEMA_EXTERNO = 'CTB'
GROUP BY MC.RUBROCONTABLE, PC.C6326, MC.FECHAPROCESO, MC.DEBITOCREDITO, TT.DESCRIPCION, E.EMPRESA , MC.OPERACION, MC.COD_TRANSACCION</sql>
